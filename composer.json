{
    "name": "fadoe/symfony-asset-module",
    "description": "Use Symfony Asset as Zend Framework Module",
    "type": "Module",
    "require": {
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-servicemanager": "^2.5",
        "zendframework/zend-stdlib": "^2.5",
        "zendframework/zend-view": "^2.5",
        "symfony/asset": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "zendframework/zend-mvc": "^2.5",
        "zendframework/zend-serializer": "^2.5",
        "zendframework/zend-log": "^2.5",
        "zendframework/zend-i18n": "^2.5"
    },
    "authors": [
        {
            "name": "Falk Doering",
            "email": "falk.doering@fadoe.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "FaDoe\\SymfonyAssetModule\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FaDoe\\SymfonyAssetModule\\": "tests/"
        }
    }
}
